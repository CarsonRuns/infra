    <validationRules>
        <fullName>Billing_Model_on_Recurring_charge</fullName>
        <active>true</active>
        <description>If Recurring Charge ($) is not blank, then Bliiling Model is a required field.</description>
        <errorConditionFormula>Recurring_Charge__c &gt; 0 &amp;&amp; ISPICKVAL( Billing_Model__c ,"")</errorConditionFormula>
        <errorMessage>Billing Model is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Offer_Cant_GT_Expected_Products</fullName>
        <active>true</active>
        <description>Validate : if (Product Offer Value ($)/1000) &gt; (Opportunity Expected Product)</description>
        <errorConditionFormula>IF(
	AND(ISBLANK(Opportunity_Name__r.Expected_Product__c), NOT(ISBLANK(Product_Offer_Value__c))),
	true,
	IF(( Product_Offer_Value__c / 1000)&gt;(Opportunity_Name__r.Expected_Product__c),
		true,
		false
	)
)</errorConditionFormula>
        <errorMessage>Product Offer Value cannot be greater than the opportunity Expected Product amount.  Please adjust the Product Offer Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Offer_Cant_GT_Expected_Service</fullName>
        <active>true</active>
        <description>Validate : if (Service Offer Value ($)/1000) &gt; (Opportunity Expected Service)</description>
        <errorConditionFormula>IF(
	AND(ISBLANK(Opportunity_Name__r.Expected_Service__c), NOT(ISBLANK(Service_Offer_Value__c))),
	true,
	IF(( Service_Offer_Value__c / 1000)&gt;(Opportunity_Name__r.Expected_Service__c),
		true,
		false
	)
)</errorConditionFormula>
        <errorMessage>Service Offer Value cannot be greater than the opportunity Expected Service amount.  Please adjust the Service Offer Value.</errorMessage>
    </validationRules>
