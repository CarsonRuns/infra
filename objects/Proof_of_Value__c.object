) 


/*IF (
    TEXT(eSTAR_Order_Status__c)= 'OVERDUE', 
        'Open',

    IF(TEXT(eSTAR_Order_Status__c)= 'PARTIALLY RETURNED', 
        'Open',

    IF(TEXT(eSTAR_Order_Status__c)= 'PARTIALLY SHIPPED', 
        'Open',

