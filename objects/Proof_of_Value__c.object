    <webLinks>
        <fullName>Send_EULA_Submit_Approval</fullName>
        <availability>online</availability>
        <description>Send EULA/Submit Approval</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send EULA/Submit Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT("/soap/ajax/30.0/connection.js")} 
{!REQUIRESCRIPT("/soap/ajax/30.0/apex.js")} 
{!REQUIRESCRIPT("/apex/dsfs__DocuSign_JavaScript")} 
//DST is the template ID 
var DST='fbf28342-c097-428c-89e6-5d09fd378c40'; 

//CCRM is the custom role mapping. It’s similar to a key:value pair where the key is the Salesforce role and value is the DocuSign template role. In this example, I’m using “Custom Role” and “Custom Role2” to say that I’m not using any predefined SFDC roles. 

var CCRM='CustomRole~Customer'; 

//CRL is the custom recipient list. On this template I have 2 recipients, and describe their properties here, per role. Note that you can use queries to pull in values from your objects like I have done in the first e-mail address, pulling the opp owner’s e-mail from the record. 

var CRL='Email~{!Proof_of_Value__c.POV_Contact_Email__c};LastName~{!Proof_of_Value__c.PoV_Contact_Last_Name__c};FirstName~{!Proof_of_Value__c.PoV_Contact_First_Name__c};Role~CustomRole'; 

//This effectively sends a redirect executing a call to DS. Note at the end the “OCO=Send” variable, which allows you to do a one-click send without the need to go into the template / tagging view. 

if("{!Proof_of_Value__c.PoV_Order_Category__c}" == 'POV Security' &amp;&amp; "{!Proof_of_Value__c.POV_Agreement_Exception__c}" != 'Yes'){
if("{!Proof_of_Value__c.Opportunity_Workspace__c}" == null || "{!Proof_of_Value__c.Opportunity_Workspace__c}" == '' ){ 
alert('In order to Submit a Security PoV for Approval, you must create and assign an Opportunity Workspace to the PoV record.'); 
} 
else{ 
if("{!Proof_of_Value__c.EULA_Status__c}" == 'Completed'){ 
alert('Customer has already accepted POV Agreement'); 
} 

else { 
window.location.href = "/apex/dsfs__DocuSign_CreateEnvelope?DSEID=0&amp;SourceID={!Proof_of_Value__c.Id}&amp;DST="+DST+"&amp;CCRM="+CCRM+"&amp;CRL="+CRL+"&amp;OCO=Send"; 
} 
} 
} 
else if("{!Proof_of_Value__c.PoV_Order_Category__c}" == 'POV Security' &amp;&amp; "{!Proof_of_Value__c.POV_Status__c}" != 'Approval In Progress' &amp;&amp; "{!Proof_of_Value__c.POV_Status__c}" != 'POV Approved' &amp;&amp; "{!Proof_of_Value__c.POV_Agreement_Exception__c}" == 'Yes'){ 
if("{!Proof_of_Value__c.Opportunity_Workspace__c}" == null || "{!Proof_of_Value__c.Opportunity_Workspace__c}" == '' ){ 
alert('In order to Submit a Security PoV for Approval, you must create and assign an Opportunity Workspace to the PoV record.'); 
} 
else{ 
var povID = "{!Proof_of_Value__c.Id}"; 
var result = sforce.apex.execute("AutoPopulateManager","submitForApproval_NonSec",{recID : povID}); 
window.location=window.location; 
}	
} 
else if("{!Proof_of_Value__c.PoV_Order_Category__c}" != 'POV Security' &amp;&amp; "{!Proof_of_Value__c.POV_Status__c}" != 'Approval In Progress' &amp;&amp; "{!Proof_of_Value__c.POV_Status__c}" != 'POV Approved'){ 
var povID = "{!Proof_of_Value__c.Id}"; 
var result = sforce.apex.execute("AutoPopulateManager","submitForApproval_NonSec",{recID : povID}); 
window.location=window.location; 
} 
else{ 
if(("{!Proof_of_Value__c.POV_Status__c}" == 'POV Approved' || "{!Proof_of_Value__c.POV_Status__c}" == 'Approval In Progress')){ 
alert('POV has already been Approved or Approval is in Progress'); 
} 
}</url>
