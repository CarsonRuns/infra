        <url>{!REQUIRESCRIPT("/soap/ajax/20.0/connection.js")}
var baseUrl = getConstant('New Activity TheaterId','URL');
var Theater = getUserTheater() || '--None--';
var org_id = '{!$Organization.Id}'; 
var opptyMultiNational = '{!Opportunity.Emerging_Multinational_Opportunity__c}';
var opptyName = escape('{!Opportunity.Name}');
var url = "/00U/e?&amp;who_id={!Opportunity.Id}&amp;what_id={!Opportunity.Id}&amp;&amp;retURL=/{!Opportunity.Id}";
if(opptyMultiNational != '--None--' &amp;&amp; opptyMultiNational != '' &amp;&amp; opptyMultiNational != null) 
{
url = url + "&amp;" + baseUrl + "=" + Theater + "&amp;00N30000001VfgP=Multinational Accounts" + "&amp;tsk4={!TODAY()}" + "&amp;tsk5="+opptyName+"&amp;00N30000001FZej={!Opportunity.DealID__c}"+"&amp;nooverride=1"+"&amp;email=1";
}
else
{
url = url + "&amp;" + baseUrl + "=" + Theater + "&amp;nooverride=1";
}
parent.location.href = url;
function getUserTheater(){ 
var qrStr = "Select Theatre__c From User where "+"Id = '{!$User.Id}' "; 
var queryResult=sforce.connection.query(qrStr); 
var records = queryResult.getArray('records'); 
return records[0].Theatre__c; 
} 
function getConstant(i_key, i_oType) 
{ 
var qrStr = "SELECT prod_value__c, non_prod_value__c " + 
"FROM Org_Constants__c " + 
"WHERE Key__c = '" + i_key + "'"; 

try { 
var queryResult=sforce.connection.query(qrStr); 
var records = queryResult.getArray('records'); 
var q_value; 
if (records.length &gt; 0) 
{ 
var hostname = window.location.hostname.toLowerCase(); 
//org_id = '00D300000000QUu'; 
if (org_id == '00D300000000QUu' &amp;&amp; ((hostname.indexOf('ww') &amp;&amp; hostname.indexOf('na')) != -1)) 
{ 
q_value = records[0].prod_value__c; 
} 
else 
{ 
q_value = records[0].non_prod_value__c; 
} 
} 
else if (records.length == 0) 
alert('No value defined for ' + i_key +', ' + i_oType); 

if (q_value == null) alert(i_key +', ' + i_oType + ' has a null value'); 
return q_value; 
} 
catch(ex) { 
alert(ex); 
} 
}</url>
