        <fullName>Customer_Objective_Plan__c</fullName>
        <description>Customer Objective/Plans of related opportunity</description>
        <externalId>false</externalId>
        <formula>TRIM(IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Improve Operation Efficiency'),' Improve Operation Efficiency '+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Consolidate-Application Workloads'),' Consolidate-Application Workloads'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Reduce Overall Data Center Cost'),' Reduce Overall Data Center Cost'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Reduce Complexity'),' Reduce Complexity'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Address Staffing Cost Issues listed'),' Address Staffing Cost Issues listed'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'New Project'),' New Project'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Disaster Recovery'),' Disaster Recovery'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Application Implementation'),' Application Implementation'+BR(),'') +
IF( INCLUDES(Opportunity__r.Customer_Objective_Plans__c,'Other'),' Other',''))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Customer Objective/Plan</label>
        <required>false</required>
