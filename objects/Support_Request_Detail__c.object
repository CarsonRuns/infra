    <validationRules>
        <fullName>Comments_for_Rejected_Status</fullName>
        <active>true</active>
        <description>This rule will validate that Support Provider should enter the comments if he has canceled or rejected the request</description>
        <errorConditionFormula>AND( 
ISBLANK(Comments__c ) ,
ISPICKVAL( Status__c , 'Rejected')
)</errorConditionFormula>
        <errorMessage>Please fill the Comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Edit_of_Completed_Request</fullName>
        <active>true</active>
        <description>This Validation rule will prevent edit of Request which is completed.</description>
        <errorConditionFormula>!ISNEW() &amp;&amp; 
$Profile.Id != '00e30000000dcbg' &amp;&amp; 
$Profile.Id != '00e30000000dfUE' &amp;&amp; 
$Profile.Id != '00e30000000eca0' &amp;&amp; 
$Profile.Id != '00e30000000dcbf' &amp;&amp; 
$Profile.Id != '00e30000000qiJj' &amp;&amp; 
(TEXT(PRIORVALUE(Status__c )) = 'Completed' || 
TEXT(PRIORVALUE(Status__c )) = 'Rejected' ||
TEXT(PRIORVALUE(Status__c )) = 'Completed - NS')</errorConditionFormula>
        <errorMessage>Completed or Rejected Sales Support Request Cannot be Edited</errorMessage>
    </validationRules>
